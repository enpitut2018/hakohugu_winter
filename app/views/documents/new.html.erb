<div class="container">
    <div class="row">        
        <div class="col-md-12">
        <h1> 新しいアシスタントとのトークを作成します </h1>
            <%= form_for(@document) do |f| %>

            <%= f.label :タイトル %>
            <%= f.text_field :title, class: 'form-control' %>
            
            <% @document.user_id = 1 %>
            <%= f.hidden_field :user_id%>

            <%= f.label :テンプレートid %>
            <%= f.text_field :template_id, class: 'form-control' %>

            <%= f.submit "アシスタントとトークを開始", class: "btn btn-primary" %>
            <% end %>
        </div>
    </div>
</div>